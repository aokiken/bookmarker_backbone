h3 Bookmarks
table(cellpadding="0" cellspacing="0")
  thead
    tr
      th Id
      th Title
      th Created
      th Modified
      th.actions Actions
  tbody
    <% _.each(bookmarks, function(bookmark) { %>
    tr
      td <%= bookmark.attributes.id %>
      td <%= bookmark.attributes.title %>
      td <%= bookmark.dateFormat("created") %>
      td <%= bookmark.dateFormat("modified") %>
      td.actions
        a(href!="#bookmarks/view/<%= bookmark.attributes.id %>") View
        a(href!="#bookmarks/edit/<%= bookmark.attributes.id %>") Edit
        a(href!="#bookmarks/delete/<%= bookmark.attributes.id %>") Delete
    <% }); %>
.paginator
  ul.pagination
    li.prev.disabled
      a(href="" onclick="return false;") &lt; previous
    li.next.disabled
      a(href="" onclick="return false;") next &gt;
  p 1 of 1